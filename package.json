{
  "name": "dese-cra",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "babyparse": "^0.4.6",
    "jquery": "^3.2.1",
    "lodash": "^4.17.11",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-share": "^1.19.1",
    "react-sticky": "^6.0.2",
    "recharts": "^1.4.2"
  },
  "devDependencies": {
    "csvtojson": "^2.0.10",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-json": "^1.3.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "prettier": "1.14.2",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules/ --include-path ./node_modules/ ./src -o ./src --watch",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint .",
    "lint-fix": "./node_modules/.bin/eslint --fix .; exit 0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
